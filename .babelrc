{
  "env": {
    "test": {
      "presets": ["@babel/preset-env"]
    },
    "build": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["ie >= 11"]
            },
            "modules": false,
            "useBuiltIns": "usage",
            "corejs": "3.2"
          }
        ]
      ]
    }
  },
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]]
}
